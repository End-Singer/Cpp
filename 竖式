#include<bits/stdc++.h>
using namespace std;
int main(){
	int num,single,ten,hun;
	cout<<"请输入一个三位数" ;
	cin>>num;
	
	hun=num/100;
	ten=(num/10)%10;
	single=(num%100)%10;
	cout<<hun*100+ten*10+single<<endl;
	cout<<hun*100+single*10+ten<<endl;
	cout<<single*100+ten*10+hun<<endl;
	cout<<single*100+hun*10+ten<<endl;
	cout<<ten*100+hun*10+single<<endl;
	cout<<ten*100+single*10+hun<<endl;
	return 0;
}
