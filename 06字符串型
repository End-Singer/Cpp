#include<bits/stdc++.h>
using namespace std;
int main(){
	// C风格的字符串 
	// 字符串型 char 变量名[] = "字符串值" （双引号） 
	char str[] = "hello world";
	cout << str << endl;
	
	
	// C++风格的字符串
	//不用万能头的时候需要用#include <string>头文件 
	// string 变量名 = "字符串值" 
	string str1 = "perfect";
	cout << str1 << endl;
	

	return 0;
}
